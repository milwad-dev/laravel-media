{
    "name": "milwad/laravel-media",
    "description": "Upload your media very easy",
    "keywords": ["laravel", "php", "composer", "laravel-media", "milwad", "laravel-media-uploader"],
    "minimum-stability": "dev",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "milwad-dev",
            "email": "milwad.dev@gmail.com",
            "role": "owner",
            "homepage": "https://github.com/milwad-dev"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Milwad\\LaravelMedia\\LaravelMediaServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0",
        "phpunit/phpunit": "^9.4"
    },
    "autoload": {
        "psr-4": {
            "Milwad\\LaravelMedia\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Milwad\\LaravelMedia\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
